  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^18.16.3",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
